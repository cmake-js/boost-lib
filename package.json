{
  "name": "boost-lib",
  "version": "0.10.0",
  "description": "Boost dependency manager for CMake.js based native modules",
  "license": "MIT",
  "keywords": [
    "native",
    "addon",
    "module",
    "c",
    "c++",
    "bindings",
    "build",
    "cmake",
    "nw.js",
    "boost"
  ],
  "author": "Gábor Mező aka unbornchikken",
  "repository": {
    "type": "git",
    "url": "git://github.com/unbornchikken/boost-lib.git"
  },
  "bin": "./bin/boost-lib",
  "main": "./lib/index.js",
  "engines": {
    "node": ">= 0.10.0",
    "iojs": ">= 1.0.0"
  },
  "dependencies": {
    "bluebird": "*",
    "cmake-js": "2",
    "debug": "*",
    "fs-extra": "0",
    "lodash": "*",
    "memory-stream": "0",
    "npmlog": "1",
    "request": "2",
